{
  "name": "chrislagasse.com",
  "version": "1.0.0",
  "description": "Personal website for Chris Lagasse, using the 11ty base by Zach Leatherman",
  "scripts": {
    "build": "npm run sass && eleventy",
    "watch": "npm-run-all sass --parallel watch:*",
    "watch:eleventy": "eleventy --serve --watch",
    "watch:sass": "npm run sass -- --watch",
    "debug": "DEBUG=* eleventy --serve --watch",
    "sass": "sass --style=compressed src/scss/main.scss css/index.css"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/soben/chrislagasse.com.git"
  },
  "author": {
    "name": "Chris Lagasse",
    "email": "chrislagasse@protonmail.com",
    "url": "https://chrislagasse.com/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/soben/chrislagasse.com/issues"
  },
  "homepage": "https://github.com/soben/chrislagasse.com#readme",
  "devDependencies": {
    "@11ty/eleventy": "^0.10.0",
    "@11ty/eleventy-navigation": "^0.1.3",
    "@11ty/eleventy-plugin-rss": "^1.0.7",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.1",
    "luxon": "^1.21.3",
    "markdown-it": "^8.4.2",
    "markdown-it-anchor": "^5.2.5",
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {
    "sass": "^1.26.5"
  }
}
